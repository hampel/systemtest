{
    "name": "hampel/systemtest",
	"homepage" : "https://bitbucket.org/hampel/laravel-system-test",
    "description": "Laravel package for testing or troublshooting various subsystems",
	"authors": [{
		"name": "Simon Hampel",
		"email": "simon@hampelgroup.com",
		"role": "Developer"
	}],
	"support": {
		"issues": "https://bitbucket.org/hampel/laravel-system-test/src",
		"source": "https://bitbucket.org/hampel/laravel-system-test/issues"
	},
	"keywords" : [
		"laravel",
		"email",
		"cache",
		"filesystem",
		"logging",
		"notifications",
		"test"
	],
	"license" : [
		"MIT"
	],
	"require": {
		"illuminate/support": "^8.0|^9.0|^10.0"
	},
	"autoload": {
		"psr-4": {
			"Hampel\\SystemTest\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Hampel\\SystemTest\\SystemTestServiceProvider"
			]
		}
	},
    "suggest": {
        "laravel/slack-notification-channel": "Required to use the slack transport."
    },
 	"minimum-stability": "dev",
	"prefer-stable": true
}
